<section>
    <div class="carousel">
        <ul class="slides">
            <input type="radio" name="radio-buttons" id="img-1" checked />
            <li class="slide-container">
                <div class="slide-image m-cover">
                    <div class="div-cover-img m-cover">
                        <img src="~/assets/img/Dining/dapitanbar/03 Dapitan bar/20220318-MEB_4639.jpg" class="f-and-b m-cover" alt="dining" />
                    </div>
                </div>
                <div class="carousel-controls">
                    <label for="img-3" class="prev-slide">
                        <span>&lsaquo;</span>
                    </label>
                    <label for="img-2" class="next-slide">
                        <span>&rsaquo;</span>
                    </label>
                </div>
            </li>
            <input type="radio" name="radio-buttons" id="img-2" />
